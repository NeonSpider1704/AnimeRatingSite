@model IEnumerable<AnimeRatingSite.Models.Anime>
@{
    ViewData["Title"] = "Browse by Genre";
}
<h1>You are looking at @ViewData["GenreName"] Animes</h1>

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item">
            <a asp-controller="Animes" asp-action="Index">Animes</a>
        </li>
        <li class="breadcrumb-item active">
            @ViewData["GenreName"]
        </li>
    </ol>
</nav>

<ul>
    @foreach(var anime in Model){
        <li>@anime.Title</li>
    }
</ul>

<a class="btn btn-dark" href="javascript:history.go(-1)"><-- Back</a>
